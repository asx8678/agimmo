{
	"name": "agimmo",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:cf": "pnpm build && wrangler dev --local --persist-to .wrangler/state",
		"build": "vite build",
		"preview": "vite preview",
		"deploy": "pnpm build && wrangler deploy",
		"seed:r2-images": "node scripts/seed-r2-images.mjs",
		"db:generate": "drizzle-kit generate",
		"db:migrate:local": "wrangler d1 migrations apply agi-db --local",
		"db:migrate:remote": "wrangler d1 migrations apply agi-db --remote",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251216.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/leaflet": "^1.9.21",
		"drizzle-kit": "^0.31.8",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"wrangler": "^4.54.0"
	},
	"dependencies": {
		"@stripe/stripe-js": "^8.6.0",
		"drizzle-orm": "^0.45.1",
		"flowbite": "^4.0.1",
		"flowbite-svelte": "^1.30.1",
		"flowbite-svelte-icons": "^3.0.1",
		"leaflet": "^1.9.4",
		"stripe": "^20.0.0",
		"svelte-kit-connect-cloudflare-kv": "^0.1.1",
		"svelte-kit-sessions": "^0.4.0",
		"sveltekit-superforms": "^2.29.1",
		"zod": "^4.2.1"
	}
}
